<!-- Edit collection form !-->
<form class="changeModel" id="{{name}}">
  <h5>Upload a 3D track model:</h5>

  <div class="input-group">
    <span class="input-group-addon">Model</span>
    <input type="hidden" name="collectionName" value="{{name}}">
    <!-- Browse file button !-->
    <input type="text" class="form-control" value="{{model}}" readonly>
    <span class="input-group-btn">
      <span class="btn btn-default btn-file">
        <i class="fa fa-folder"></i>
        <input type="file" name="file">
      </span>
    </span>
    <span class="input-group-btn">
      <span class="btn btn-default btn-upload-model" data-collectionName="{{name}}">
        <i class="fa fa-upload"></i>
      </span>
    </span>
  </div>
  <div class="alert alert-danger alert-dismissible collapse" role="alert"></div>
</form>
