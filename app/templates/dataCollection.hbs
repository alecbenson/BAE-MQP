<div class="panel panel-default">

  <!-- Panel heading !-->
  <div class="panel-heading clearfix">
    {{> panelSettings}}
    <span data-toggle="collapse" data-target=".{{name}}">
      {{name}}
    </span>
  </div>

  <!-- Panel contents !-->
  <div class="panel-collapse collapse in {{name}}">
    <div class="panel-body">
      {{> sourceList}}
      {{> sourceUpload}}
    </div>
  </div>
</div>
