<div class="panel panel-default">
  <!-- Panel heading !-->
  <div class="panel-heading clearfix">
    <span data-toggle="collapse" data-target=".filter-ele-body">Elevation Filter</span>
    <span class="pull-right filter-ele-enabled">
      <input class="btn-visibility" data-size="mini" type="checkbox" checked data-toggle="toggle" data-on="On" data-off="Off">
    </span>
  </div>
  <!-- Panel contents !-->
  <div class="panel-collapse collapse in filter-ele-body">
    <div class="panel-body">
      {{> numberpicker label='Min' unit='Meters' id='filter-ele-start'}}
      <br>
      {{> numberpicker label='Max' unit='Meters' id='filter-ele-stop'}}
    </div>
  </div>
</div>
