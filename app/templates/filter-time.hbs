<div class="panel-group">
  <div class="panel panel-default">
    <div class="panel-heading clearfix">
      <span class="accordion" data-toggle="collapse" data-target="#filter-time-body">
        <h4>Time Filter</h4>
      </span>
      <span class="navbar-right filter-time-enabled">
        <input class="btn-visibility" data-size="mini" type="checkbox" checked data-toggle="toggle" data-on="On" data-off="Off">
      </span>
    </div>

    <div class="panel-collapse collapse in" id="filter-time-body">
      <div class="panel-body">
        {{> datepicker label='Start Time' id='filter-time-start'}} {{> datepicker label='Stop Time' id='filter-time-stop'}}
      </div>
    </div>
  </div>
</div>
